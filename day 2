
class keyboard 
{
    int keys;
    String color;

    public keyboard()
    {
        keys=100;
        color="white";
    }
    public void pressed() {
        System.out.println("Signal sent" + color);
    }

    public void throwIt() {
   
        System.out.println("got hit");
        keys=85;
    }

}

class Demo
{
    public static void main(String args[])
    {
    
        keyboard obj;
        obj = new keyboard();

        obj.pressed();
        obj.throwIt();
    
        System.out.println(obj.keys);
    }
}

